<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="cn.eas">
	<class
		name="Projectdb"
		table="projectdb"
	>
		<meta attribute="sync-DAO">true</meta>
		<id
			name="Id"
			type="long"
			column="id"
		>
			<generator class="identity"/>
		</id>

		<property
			name="Date"
			column="date"
			type="timestamp"
			not-null="false"
			length="19"
		/>
		<property
			name="Name"
			column="name"
			type="string"
			not-null="true"
			length="45"
		/>


	</class>	
</hibernate-mapping>